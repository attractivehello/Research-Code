# %%
import networkx as nx
import random
import matplotlib.pyplot as plt
from graphUtils import create_layout, graph_attributes_selection, update_graph_attributes

# %%
G = nx.read_graphml("basegraph.graphml")
grid_pos = create_layout(G)

plt.figure(3,figsize=(12,12))
nx.draw(G, pos = grid_pos, with_labels = True)
plt.show()

# %%
graph_attributes_selection(G,1998)

# %%
nx.write_graphml_lxml(G, "basegraphattributes.graphml")

